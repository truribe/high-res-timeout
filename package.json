{
  "name": "high-res-timeout",
  "version": "1.0.8",
  "description": "A high-resolution, object-oriented replacement for setTimeout() and setInterval(). Good for animations, progress indicators, carousels, you name it.",
  "main": "dist/index.js",
  "watch": {
    "cover": [
      "src/**/*.js",
      "test/**/*.js"
    ]
  },
  "scripts": {
    "clean": "rimraf dist",
    "watch": "npm-watch cover",
    "serve": "browser-sync start -s -f=coverage/lcov-report/**/*.html --startPath=coverage/lcov-report/index.html",
    "test": "mocha --require @babel/register --require @babel/polyfill --recursive",
    "test:prod": "cross-env BABEL_ENV=production npm run test",
    "test:examples": "node examples/",
    "cover": "istanbul cover ./node_modules/mocha/bin/_mocha -- --require babel-core/register --require babel-polyfill --recursive",
    "lint": "eslint src test",
    "build": "cross-env BABEL_ENV=production babel src --out-dir dist",
    "prepublish": "npm run clean && npm run lint && npm run test && npm run build"
  },
  "repository": "github:truribe/high-res-timeout",
  "keywords": [
    "js",
    "timer",
    "timeout",
    "javascript",
    "setTimeout",
    "setInterval",
    "performance.now",
    "DOMHighResTimeStamp",
    "requestAnimationFrame"
  ],
  "author": "Travis Uribe <travis@turibe.com> (http://turibe.com)",
  "license": "ISC",
  "homepage": "https://github.com/truribe/high-res-timeout",
  "dependencies": {
    "events": "^3.1.0",
    "performance-now": "^2.1.0",
    "raf": "^3.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "@babel/register": "^7.9.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "browser-sync": "^2.26.7",
    "chai": "^4.2.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "istanbul": "^0.4.5",
    "mocha": "^7.1.2",
    "npm-watch": "^0.6.0",
    "rimraf": "^3.0.2"
  }
}
